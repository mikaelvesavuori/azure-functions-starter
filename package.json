{
  "name": "azure-functions-starter",
  "version": "1.0.0",
  "author": "Mikael Vesavuori",
  "description": "Azure Functions starter with Typescript, Webpack, Serverless Framework and Jest",
  "scripts": {
    "start": "sls offline",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "npm run build && npm run test:integration && npm run test:unit",
    "test:integration": "node __tests__/runIntegrationTests.mjs",
    "test:unit": "jest __tests__ --collectCoverage",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix"
  },
  "dependencies": {
    "@azure/functions": "^1.2.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/types": "^7.10.4",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "serverless-azure-functions": "^2.0.13",
    "serverless-webpack": "^5.3.2",
    "superagent": "^5.3.1",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
